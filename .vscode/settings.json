{
    "editor.fontFamily": "FiraCode, Menlo, Monaco, 'Courier New', monospace",
    "code-runner.executorMap": {
        "javascript": "node",
        "typescript": "ts-node --files"
    },
    "eslint.format.enable": true,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
    
}